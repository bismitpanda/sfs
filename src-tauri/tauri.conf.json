{
    "build": {
        "beforeBuildCommand": "yarn build",
        "beforeDevCommand": "yarn dev",
        "devPath": "http://localhost:3333",
        "distDir": "../dist"
    },
    "package": {
        "productName": "app",
        "version": "0.1.0"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "process": {
                "all": false,
                "exit": true,
                "relaunch": false,
                "relaunchDangerousAllowSymlinkMacos": false
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "sfs",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 720,
                "resizable": true,
                "title": "Secure File System",
                "width": 1280,
                "minHeight": 600,
                "minWidth": 1000,
                "center": true
            }
        ]
    }
}
