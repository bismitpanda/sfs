{
    "build": {
        "beforeBuildCommand": "pnpm build",
        "beforeDevCommand": "pnpm dev",
        "devPath": "http://localhost:2004",
        "distDir": "../dist"
    },
    "package": {
        "productName": "sfs-ui",
        "version": "0.1.0"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "process": {
                "exit": true
            },
            "dialog": {
                "open": true,
                "save": true
            },
            "window": {
                "all": false,
                "close": true,
                "hide": true,
                "show": true,
                "maximize": true,
                "minimize": true,
                "unmaximize": true,
                "unminimize": true,
                "startDragging": true
            },
            "clipboard": {
                "writeText": true
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "sfs",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 720,
                "resizable": true,
                "title": "Secure File System",
                "width": 1280,
                "minHeight": 600,
                "minWidth": 1000,
                "center": true,
                "visible": false,
                "label": "main",
                "decorations": false,
                "url": "main.html",
                "contentProtected": true
            },
            {
                "width": 480,
                "height": 200,
                "decorations": false,
                "transparent": true,
                "url": "login.html",
                "label": "login",
                "center": true,
                "resizable": false,
                "focus": true
            }
        ]
    }
}
